{
  "$id": "PostCheckCodeErrorSchema",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Post Check Code Error Schema",
  "type": "object",

  "properties": {
    "status": {
      "type": "string",
      "const": "error"
    },
    "result": {
      "type": "object",

      "properties": {
        "code": {
          "type": "object",
          "properties": {
            "status": {
              "type": "string",
              "enum": ["success", "error", "notExists"]
            }
          },
          "required": ["status"]
        }
      },
      "required": ["code"]
    }
  },
  "required": ["status", "result"]
}
