{
  "$id": "postRefreshTokenErrorSchema",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "postRefreshTokenErrorSchema",
  "type": "object",

  "properties": {
    "status": {
      "type": "string",
      "const": "success"
    },
    "result": {
      "type": "object",

      "properties": {
        "token": {
          "type": "object",

          "properties": {
            "error": {
              "type": "string",
              "const": "invalid_request"
            },
            "error_description": {
              "type": "string"
            },
            "hint": {
              "type": "string"
            },
            "message": {
              "type": "string"
            }
          },
          "required": ["error", "error_description", "hint", "message"]
        }
      },
      "required": ["token"]
    }
  },
  "required": ["status", "result"]
}
