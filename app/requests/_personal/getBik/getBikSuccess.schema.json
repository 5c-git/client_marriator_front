{
  "$id": "getBikSuccess",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "getBikSuccess",
  "type": "object",

  "properties": {
    "result": {
      "type": "object",

      "properties": {
        "bankData": {
          "type": "array",
          "items": {
            "type": "object",

            "properties": {
              "value": {
                "type": "string"
              },
              "bic": {
                "type": "string"
              },
              "label": {
                "type": "string"
              },
              "disabled": {
                "type": "boolean"
              }
            },
            "required": ["value", "bic", "label", "disabled"]
          }
        }
      },
      "required": ["bankData"]
    },
    "status": {
      "type": "string",
      "const": "success"
    }
  },
  "required": ["result", "status"]
}
