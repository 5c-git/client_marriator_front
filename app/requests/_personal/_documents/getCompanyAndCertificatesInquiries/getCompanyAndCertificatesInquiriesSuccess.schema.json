{
  "$id": "getCompanyAndCertificatesInquiriesSuccess",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "getCompanyAndCertificatesInquiriesSuccess",
  "type": "object",

  "properties": {
    "status": {
      "type": "string",
      "const": "success"
    },
    "result": {
      "type": "object",

      "properties": {
        "organization": {
          "type": "array",
          "items": {
            "type": "object",

            "properties": {
              "uuid": {
                "type": "string"
              },
              "name": {
                "type": "string"
              }
            },
            "required": ["uuid", "name"]
          }
        },
        "certificates": {
          "type": "array",
          "items": {
            "type": "object",

            "properties": {
              "id": {
                "type": "number"
              },
              "key": {
                "type": "string"
              },
              "value": {
                "type": "string"
              }
            },
            "required": ["id", "key", "value"]
          }
        }
      },
      "required": ["organization", "certificates"]
    }
  },
  "required": ["status", "result"]
}
