{
  "$id": "getOrderSuccess",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "getOrderSuccess",
  "type": "object",

  "properties": {
    "data": {
      "type": "object",

      "properties": {
        "id": {
          "type": "number"
        },
        "selfEmployed": {
          "type": "boolean"
        },
        "status": {
          "enum": [1, 2, 3, 4, 5]
        },
        "place": {
          "type": "object",

          "properties": {
            "id": {
              "type": "number"
            },
            "name": {
              "type": "string"
            },
            "latitude": {
              "type": "string"
            },
            "longitude": {
              "type": "string"
            },
            "address_kladr": {
              "type": "string"
            },
            "logo": {
              "type": "string"
            },
            "region": {
              "type": "object",

              "properties": {
                "id": {
                  "type": "number"
                },
                "name": {
                  "type": "string"
                }
              },
              "required": ["id", "name"]
            },
            "brand": {
              "type": "object",

              "properties": {
                "id": {
                  "type": "number"
                },
                "name": {
                  "type": "string"
                },
                "logo": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                }
              },
              "required": ["id", "name", "logo", "description"]
            }
          },
          "required": [
            "id",
            "name",
            "latitude",
            "longitude",
            "address_kladr",
            "logo",
            "region",
            "brand"
          ]
        },
        "user": {
          "type": "object",

          "properties": {
            "id": {
              "type": "number"
            },
            "phone": {
              "type": "number"
            },
            "email": {
              "type": "string"
            },
            "logo": {
              "type": "string"
            },
            "roles": {
              "type": "array",
              "items": {
                "type": "object",

                "properties": {
                  "id": {
                    "type": "number",
                    "minimum": 1,
                    "maximum": 6
                  },
                  "name": {
                    "enum": [
                      "admin",
                      "client",
                      "manager",
                      "recruiter",
                      "specialist",
                      "supervisor"
                    ]
                  }
                },
                "required": ["id", "name"]
              }
            }
          },
          "required": ["id", "phone", "email", "logo", "roles"]
        },
        "orderActivities": {
          "type": "array",
          "items": {
            "type": "object",

            "properties": {
              "viewActivity": {
                "type": "object",

                "properties": {
                  "id": {
                    "type": "number"
                  },
                  "name": {
                    "type": "string"
                  },
                  "detailName": {
                    "type": "string"
                  },
                  "previewText": {
                    "type": "string"
                  },
                  "logo": {
                    "type": "string"
                  },
                  "traveling": {
                    "type": "boolean"
                  }
                },
                "required": [
                  "id",
                  "name",
                  "detailName",
                  "previewText",
                  "logo",
                  "traveling"
                ]
              },
              "id": {
                "type": "number"
              },
              "count": {
                "type": "number"
              },
              "dateStart": {
                "type": "string"
              },
              "dateEnd": {
                "type": "string"
              },
              "needFoto": {
                "type": "boolean"
              },
              "dateActivity": {
                "type": "array",
                "items": {
                  "type": "object",

                  "properties": {
                    "timeStart": {
                      "type": "string"
                    },
                    "timeEnd": {
                      "type": "string"
                    },
                    "places": {
                      "type": "array",
                      "items": {
                        "type": "object",

                        "properties": {
                          "id": {
                            "type": "number"
                          },
                          "name": {
                            "type": "string"
                          },
                          "latitude": {
                            "type": "string"
                          },
                          "longitude": {
                            "type": "string"
                          },
                          "address_kladr": {
                            "type": "string"
                          },
                          "logo": {
                            "type": ["null", "string"]
                          },
                          "region": {
                            "type": "object",

                            "properties": {
                              "id": {
                                "type": "number"
                              },
                              "name": {
                                "type": "string"
                              }
                            },
                            "required": ["id", "name"]
                          },
                          "brand": {
                            "type": "object",

                            "properties": {
                              "id": {
                                "type": "number"
                              },
                              "name": {
                                "type": "string"
                              },
                              "logo": {
                                "type": ["null", "string"]
                              },
                              "description": {
                                "type": "string"
                              }
                            },
                            "required": ["id", "name", "description"]
                          }
                        },
                        "required": [
                          "id",
                          "name",
                          "latitude",
                          "longitude",
                          "address_kladr",
                          "region",
                          "brand"
                        ]
                      }
                    }
                  },
                  "required": ["timeStart", "timeEnd", "places"]
                }
              }
            },
            "required": [
              "viewActivity",
              "id",
              "count",
              "dateStart",
              "dateEnd",
              "needFoto",
              "dateActivity"
            ]
          }
        },
        "acceptUser": {
          "type": ["null", "object"],

          "properties": {
            "id": {
              "type": "number"
            },
            "phone": {
              "type": "number"
            },
            "email": {
              "type": "string"
            },
            "logo": {
              "type": "string"
            }
          },
          "required": ["id", "phone", "email", "logo"]
        }
      },
      "required": [
        "id",
        "selfEmployed",
        "status",
        "place",
        "user",
        "orderActivities",
        "acceptUser"
      ]
    }
  },
  "required": ["data"]
}
