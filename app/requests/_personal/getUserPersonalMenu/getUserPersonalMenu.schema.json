{
  "$id": "getUserPersonalMenuSuccess",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "getUserPersonalMenuSuccess",
  "type": "object",

  "properties": {
    "status": {
      "type": "string",
      "const": "success"
    },
    "result": {
      "type": "object",

      "properties": {
        "section": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",

            "properties": {
              "name": {
                "type": "string"
              },
              "value": {
                "type": "number"
              },
              "notification": {
                "type": "boolean"
              }
            },
            "required": ["name", "value", "notification"]
          }
        }
      },
      "required": ["section"]
    }
  },
  "required": ["status", "result"]
}
