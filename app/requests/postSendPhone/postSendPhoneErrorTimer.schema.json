{
  "$id": "postSendPhoneErrorTimer",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "postSendPhoneErrorTimer",
  "type": "object",

  "properties": {
    "result": {
      "type": "object",

      "properties": {
        "type": {
          "type": "string"
        },
        "code": {
          "type": "object",

          "properties": {
            "status": {
              "type": "string",
              "const": "exists"
            },
            "ttl": {
              "type": "number"
            }
          },
          "required": ["status", "ttl"]
        }
      },
      "required": ["type", "code"]
    },
    "status": {
      "type": "string",
      "const": "error"
    }
  },
  "required": ["result", "status"]
}
